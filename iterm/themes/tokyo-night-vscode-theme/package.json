{
    "name": "tokyo-night",
    "displayName": "Tokyo Night",
    "description": "A clean, dark (and now light) Visual Studio Code theme that celebrates the lights of Downtown Tokyo at night.",
    "version": "0.6.7",
    "publisher": "enkia",
    "license": "MIT",
    "keywords": [
        "theme",
        "contrast",
        "dark",
        "light",
        "tokyo",
        "clean"
    ],
    "icon": "icon.png",
    "homepage": "https://github.com/enkia/tokyo-night-vscode-theme",
    "repository": {
        "url": "https://github.com/enkia/tokyo-night-vscode-theme",
        "type": "git"
    },
    "bugs": {
        "url": "https://github.com/enkia/tokyo-night-vscode-theme/issues"
    },
    "galleryBanner": {
        "color": "#1f2335",
        "theme": "dark"
    },
    "engines": {
        "vscode": "^1.17.0"
    },
    "categories": [
        "Themes",
        "Other"
    ],
    "contributes": {
        "themes": [
            {
                "label": "Tokyo Night",
                "uiTheme": "vs-dark",
                "path": "./themes/tokyo-night-color-theme.json"
            },
            {
                "label": "Tokyo Night Storm",
                "uiTheme": "vs-dark",
                "path": "./themes/tokyo-night-storm-color-theme.json"
            },
            {
                "label": "Tokyo Night Light",
                "uiTheme": "vs",
                "path": "./themes/tokyo-night-light-color-theme.json"
            }
        ]
    },
    "scripts": {
        "package": "vsce package",
        "publish": "vsce publish"
    }
}
